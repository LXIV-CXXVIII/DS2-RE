function HashWrite(value)
  local bytes = qwordToByteTable(value)
  return bytes[1],bytes[2],bytes[3],bytes[4],bytes[5],bytes[6],bytes[7],bAnd(bytes[8],0xF)
end

function CRCHash(b1,b2,b3,b4,b5,b6,b7,b8)
  return byteTableToQword({b1,b2,b3,b4,b5,b6,b7,bXor(b8,0x10)})
end

registerCustomTypeLua("HString",8,CRCHash,HashWrite,false)

function showMemAtAddress(addr)
  local mvForm = getMemoryViewForm()
  mvForm.Disassemblerview.TopAddress = getAddress(addr)
  mvForm.show()
end

